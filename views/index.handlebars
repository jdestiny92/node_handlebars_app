	<div id="dialog" title="Trainer Tip!">
	  <p>Press the pokeball to go out and catch pokemon! Press the pokedex to look at the pokemon you've caught so far!</p>
	</div>
	<script>
		new Audio('music.mp3').play();
		
		function show(){
			$('#dialog').dialog();
		}
		setTimeout(show, 3000);

		$('#mainPokeball').on('click', function(){
			location.href = 'http://localhost:3000/catch';
		})

		$('#pokedex').on('click', function(){
			location.href = 'http://localhost:3000/pokedex';
		})
	</script>